    "discord.js": "^14.9.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.1.0",
    "husky": "^8.0.1",
    "libsodium-wrappers": "^0.7.10",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.7.3",
